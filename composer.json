{
    "name": "foo.ag/packagebuilder",
    "type": "project",
    "version": "1.0.0",
    "description": "Tool to generate package.php and packages.php files for autoloading classes.",
    "authors": [
        {
            "name": "Kurt Junker",
            "email": "kurt.junker@foo.ag",
            "role": "developer"
        }
    ],
    "bin": ["bin/package-builder"],
    "autoload": {
        "psr-4": {
            "tools\\packageBuilder\\": "src/"
        }
    },
    "require": {
        "php": "7.3.*",
        "ext-tokenizer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\packageBuilder\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
